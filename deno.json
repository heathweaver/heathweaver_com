{
  "nodeModulesDir": "auto",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "dev": "deno run -A --unstable-kv vite --port 8001",
    "build": "vite build",
    "start_old": "deno run -A --unstable-kv --watch=static/,routes/ dev.ts",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update .",
    "test": "deno test -A tests/integration/ tests/unit/"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@db/": "./lib/db/",
    "@pdf-lib/fontkit": "npm:@pdf-lib/fontkit@^1.1.1",
    "$std/": "jsr:@std@1.0.0/",
    "@std/cli": "jsr:@std/cli@^1.0.0",
    "@std/yaml": "jsr:@std/yaml@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.0",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.0",
    "@b-fuze/deno-dom": "jsr:@b-fuze/deno-dom@^0.1.48",
    "postgres": "npm:postgres@^3.4.4",
    "pdf-lib": "npm:pdf-lib@^1.17.1",
    "kv-oauth": "jsr:@deno/kv-oauth",
    "oauth2": "https://deno.land/x/oauth2_client@v1.0.2/mod.ts",
    "cookie": "jsr:@std/http@1.0.0/cookie",
    "fresh": "jsr:@fresh/core@^2.1.1",
    "preact": "npm:preact@^10.27.2",
    "@preact/signals": "npm:@preact/signals@^2.3.1",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.4",
    "vite": "npm:vite@^7.1.3",
    "tailwindcss": "npm:tailwindcss@^4.1.10",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12"
  },
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  }
}
